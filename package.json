{
  "name": "dwight-job-application",
  "version": "0.0.0",
  "description": "Webcomponent dwight-job-application following open-wc recommendations",
  "author": "dwight-job-application",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:bs": "karma start karma.bs.config.js --coverage",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@chameleon-ds/button": "^1.3.5",
    "@chameleon-ds/card": "^1.1.1",
    "@chameleon-ds/input": "^1.2.1",
    "@chameleon-ds/radio": "^1.2.0",
    "@chameleon-ds/textarea": "^1.3.1",
    "@xstate/test": "^0.3.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^1.0.1",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@open-wc/testing-karma-bs": "^1.0.0",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.23.0",
    "query-selector-shadow-dom": "^0.4.2"
  }
}
